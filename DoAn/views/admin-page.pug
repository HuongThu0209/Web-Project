doctype html
html
    head
        title= title
        link(rel="stylesheet" href="/css/theme.css")
        link(rel="stylesheet" href="/bootstrap-4.1/bootstrap.min.css")
        link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css")
        link(rel="stylesheet", href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css")
        
    body
        aside.menu-sidebar.d-none.d-lg-block
            .logo
                a(href='#')
                    img(src='images/bg/logo1.png' style='width: 70px' alt='Welcom Admin')
                    |  Welcom Admin
            .menu-sidebar__content.js-scrollbar1
                nav.navbar-sidebar
                    ul.list-unstyled.navbar__list
                        li.active.has-sub
                            a(href='/admin-page')
                                i.fas.fa-tachometer-alt
                                | Trang ch&#x1EE7;
                        li
                            a(href='/adminPro')
                                i.fa.fa-plus-square(aria-hidden='true')
                                | Qu&#x1EA3;n l&iacute; s&#x1EA3;n ph&#x1EA9;m
                        li
                            a(href='/adminUser')
                                i.fa.fa-users(aria-hidden='true')
                                | Qu&#x1EA3;n l&iacute; users
                        li
                            a(href='/order')
                                i.far.fa-check-square
                                | Qu&#x1EA3;n l&iacute; &dstrok;&#x1A1;n h&agrave;ng
                        li.has-sub
                            a.js-arrow(href='#')
                                i.fas.fa-copy
                                | Pages
                            ul.list-unstyled.navbar__sub-list.js-sub-list
                                li
                                    a(href='login.html') Login
                                li
                                    a(href='register.html') Register
                                li
                                    a(href='forget-pass.html') Forget Password
        .page-container
            //- HEADER DESKTOP
            header.header-desktop
                .section__content.section__content--p30
                    .container-fluid
                        .header-wrap
                            form.form-header(action='' method='POST')
                                input.au-input.au-input--xl(type='text' name='search' placeholder='Search ...')
                                button.au-btn--submit(type='submit')
                                    i.fa.fa-search(aria-hidden='true')
                            .header-button
                                .noti-wrap
                                    .noti__item.js-item-menu
                                        i.fas.fa-comment-alt
                                        span.quantity 1
                                        
                                    .noti__item.js-item-menu
                                        i.fas.fa-envelope
                                        span.quantity 1
                                        
                                    .noti__item.js-item-menu
                                        i.fa.fa-bell(aria-hidden='true')
                                        span.quantity 3
                                        
                                .account-wrap
                                    .account-item.clearfix.js-item-menu
                                        .image
                                            img(src='images/avatar.jpg' alt='John Doe')
                                        .content
                                            p Sport Play
                                        

            .main-content
                .section__content.section__content--p30
                    .container-fluid
                        .row
                            .col-md-12
                                .overview-wrap
                                    h2.title-1 T&#x1ED5;ng quan
                                    //-
                                        <button class="au-btn au-btn-icon au-btn--blue">
                                        <i class="zmdi zmdi-plus"></i>add item</button>
                        .row.m-t-25
                            .col-sm-6.col-lg-3
                                .overview-item.overview-item--c1
                                    .overview__inner
                                        .overview-box.clearfix
                                            .icon
                                                i.zmdi.zmdi-account-o
                                            .text
                                                h2 1368
                                                span members online
                                        .overview-chart
                                            //-canvas#widgetChart1
                                            img(src="/images/bd1.JPG" alt="")
                            .col-sm-6.col-lg-3
                                .overview-item.overview-item--c2
                                    .overview__inner
                                        .overview-box.clearfix
                                            .icon
                                                i.zmdi.zmdi-shopping-cart
                                            .text
                                                h2 688
                                                span items solid
                                        .overview-chart
                                            //-canvas#widgetChart2
                                            img(src="/images/bd2.JPG" alt="")
                            .col-sm-6.col-lg-3
                                .overview-item.overview-item--c3
                                    .overview__inner
                                        .overview-box.clearfix
                                            .icon
                                                i.zmdi.zmdi-calendar-note
                                            .text
                                                h2 186
                                                span this week
                                        .overview-chart
                                            //-canvas#widgetChart3
                                            img(src="/images/bd3.JPG" alt="")
                            .col-sm-6.col-lg-3
                                .overview-item.overview-item--c4
                                    .overview__inner
                                        .overview-box.clearfix
                                            .icon
                                                i.zmdi.zmdi-money
                                            .text
                                                h2 $1386
                                                span total earnings
                                        .overview-chart
                                            //-canvas#widgetChart4
                                            img(src="/images/bd4.JPG" alt="")
                        .row
                            .col-lg-6
                                .au-card.recent-report
                                    .au-card-inner
                                        h3.title-2 Báo Cáo Gần Đây
                                        .chart-info
                                            .chart-info__left
                                                .chart-note
                                                    span.dot.dot--blue
                                                    span Sản Phẩm
                                                .chart-note.mr-0
                                                    span.dot.dot--green
                                                    span Dịch Vụ
                                            .chart-info__right
                                                .chart-statis
                                                    span.index.incre
                                                        i.zmdi.zmdi-long-arrow-up
                                                        | 25%
                                                    span.label Sản phẩm
                                                .chart-statis.mr-0
                                                    span.index.decre
                                                        i.zmdi.zmdi-long-arrow-down
                                                        | 10%
                                                    span.label Dịch vụ
                                        .recent-report__chart
                                            //-canvas#recent-rep-chart
                                            img(src="/images/bd5.JPG" alt="")
                            .col-lg-6
                                .au-card.chart-percent-card
                                    .au-card-inner
                                        h3.title-2.tm-b-5 Tỷ Lệ %
                                        .row.no-gutters
                                            .col-xl-6
                                                .chart-note-wrap
                                                    .chart-note.mr-0.d-block
                                                        span.dot.dot--blue
                                                        span Sản phẩm
                                                    .chart-note.mr-0.d-block
                                                        span.dot.dot--red
                                                        span Dịch vụ
                                            .col-xl-6
                                                .percent-chart
                                                    //-canvas#percent-chart
                                                    img(src="/images/bd6.JPG" alt="")


        //-script(src="/bootstrap-4.1/bootstrap.min.js")
        script(src="/bootstrap-4.1/popper.min.js")
        script(src="/js/Chart.bundle.min.js")
        script(src="/js/main.js")
        script(src="/js/moment.js")
