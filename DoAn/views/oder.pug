doctype html
head
  title= title
  link(rel="stylesheet" href="/css/theme.css")
  link(rel="stylesheet" href="/bootstrap-4.1/bootstrap.min.css")
  link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css")
  link(rel="stylesheet", href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css")
  
.page-wrapper
  // HEADER MOBILE
  header.header-mobile.d-block.d-lg-none
    .header-mobile__bar
      .container-fluid
        .header-mobile-inner
          a(href='#')
            img(src='images/bg/logo1.png' style='width: 70px' alt='Welcom Admin')
            |  Welcom Admin
            
          button.hamburger.hamburger--slider(type='button')
            span.hamburger-box
              span.hamburger-inner
    nav.navbar-mobile
      .container-fluid
        ul.navbar-mobile__list.list-unstyled
          li.has-sub
            a.js-arrow(href='/admin-page')
              i.fas.fa-tachometer-alt
              | Trang ch&#x1EE7;
            
          li
            a(href='/adminPro')
              i.fa.fa-plus-square(aria-hidden='true')
              | Qu&#x1EA3;n l&iacute; s&#x1EA3;n ph&#x1EA9;m
          li
            a(href='/adminUser')
              i.fa.fa-users(aria-hidden='true')
              | Qu&#x1EA3;n l&iacute; users
          li
            a(href='/order')
              i.far.fa-check-square
              | Qu&#x1EA3;n l&iacute; &dstrok;&#x1A1;n h&agrave;ng
          
              | Pages
            ul.navbar-mobile-sub__list.list-unstyled.js-sub-list
              li
                a(href='login.html') Login
              li
                a(href='register.html') Register
              li
                a(href='forget-pass.html') Forget Password
          
  // END HEADER MOBILE
  // MENU SIDEBAR
  aside.menu-sidebar.d-none.d-lg-block
    .logo
      a(href='#')
        img(src='images/bg/logo1.png' style='width: 70px' alt='Welcom Admin')
        |  Welcom Admin
    .menu-sidebar__content.js-scrollbar1
      nav.navbar-sidebar
        ul.list-unstyled.navbar__list
          li.has-sub
            a.js-arrow(href='/admin-page')
              i.fas.fa-tachometer-alt
              | Trang ch&#x1EE7;
            
          li
            a(href='/adminPro')
              i.fa.fa-plus-square(aria-hidden='true')
              | Qu&#x1EA3;n l&iacute; s&#x1EA3;n ph&#x1EA9;m
          li
            a(href='adminUser')
              i.fa.fa-users(aria-hidden='true')
              | Qu&#x1EA3;n l&iacute; users
          li.active
            a(href='/order')
              i.far.fa-check-square
              | Qu&#x1EA3;n l&iacute; &dstrok;&#x1A1;n h&agrave;ng
          li.has-sub
            a.js-arrow(href='#')
              i.fas.fa-copy
              | Pages
            ul.list-unstyled.navbar__sub-list.js-sub-list
              li
                a(href='login.html') Login
              li
                a(href='register.html') Register
              li
                a(href='forget-pass.html') Forget Password
  // END MENU SIDEBAR
  // PAGE CONTAINER
  .page-container
    // HEADER DESKTOP
    header.header-desktop
      .section__content.section__content--p30
        .container-fluid
          .header-wrap
            form.form-header(action='' method='POST')
                input.au-input.au-input--xl(type='text' name='search' placeholder='Search ...')
                button.au-btn--submit(type='submit')
                    i.fa.fa-search(aria-hidden='true')
            .header-button
                .noti-wrap
                    .noti__item.js-item-menu
                        i.fas.fa-comment-alt
                        span.quantity 1
                        
                    .noti__item.js-item-menu
                        i.fas.fa-envelope
                        span.quantity 1
                        
                    .noti__item.js-item-menu
                        i.fa.fa-bell(aria-hidden='true')
                        span.quantity 3
                        
                .account-wrap
                    .account-item.clearfix.js-item-menu
                        .image
                            img(src='images/avatar.jpg' alt='John Doe')
                        .content
                            p Sport Play
    // HEADER DESKTOP
    // MAIN CONTENT
    .main-content
      table.table
        thead.thead-dark
            tr
                th(scope='col') Mã đơn hàng
                th(scope='col') Tên khách hàng
                th(scope='col') Điện thoại
                th(scope='col') Email
                th(scope='col') Ngày lập
                th(scope='col') Tổng tiền
                th(scope='col') Tình trạng
        tbody
            tr
                th(scope='row') MDHA01
                td Trần Trung Kiên
                td 092691592
                td trungkien@gmail.com
                td 15/7/2020
                td 2350000 đ
                td Đang xử lí
